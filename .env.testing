APP_NAME="Hypixel Dynamic Signatures"
APP_ENV=testing
APP_KEY=base64:YourTestingKeyHere
APP_DEBUG=false
APP_URL=http://localhost
SIGNATURE_DEBUG_BACKGROUND=false

LOG_CHANNEL=stack
LOG_LEVEL=error
SLACK_LOG_LEVEL=error
LOG_SLACK_WEBHOOK_URL=

# This should be set in GitHub Secrets as HYPIXEL_API_KEY
HYPIXEL_API_KEY=your-hypixel-api-key-here
FORCE_HTTPS=false

# Use SQLite for testing to avoid database setup complexity
DB_CONNECTION=sqlite
DB_DATABASE=:memory:

BROADCAST_DRIVER=log
CACHE_DRIVER=array
QUEUE_CONNECTION=sync
SESSION_DRIVER=array
SESSION_LIFETIME=120

# Redis settings (not used in testing with array drivers)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=array
MAIL_HOST=localhost
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

IGNITION_THEME=dark